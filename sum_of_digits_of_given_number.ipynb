      "source": [
        "#04. To print the sum of digits in a number\n",
        "\n",
        "num = int(input(\"Enter any number: \"))\n",
        "sum = 0\n",
        "\n",
        "while num > 0:\n",
        "    rem = num % 10\n",
        "    sum += rem\n",
        "    num = num // 10\n",
        "\n",
        "print(\"The sum of the digits of the entered number is\", sum)"
      ]
    }
